{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": {
                    "version": 3,
                    "proposals": true
                },
                "modules": false,
                "loose": true
            }
        ],
        [
            "@babel/preset-typescript",
            {
                "jsxPragma": "h",
                "isTSX": true,
                "allExtensions": true
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", {
            "pragma": "Vue.h",
            "pragmaFrag": "Vue.Fragment"
        }]
    ]
}
