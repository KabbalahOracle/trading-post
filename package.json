{
    "name": "trading-post",
    "version": "1.0.0",
    "main": "src/index.ts",
    "license": "UNLICENSED",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "serve": "cross-env NODE_ENV=\"development\" TS_NODE_PROJECT=\"tsconfig.node.json\" gulp serve",
        "build": "cross-env NODE_ENV=\"development\" TS_NODE_PROJECT=\"tsconfig.node.json\" gulp build",
        "postinstall": "patch-package",
        "envoy:up": "bash -c 'docker run -dt -v $(pwd)/envoy.yaml:/etc/envoy/envoy.yaml:ro --name trading-post-mirror-envoy -p 11206:11206 envoyproxy/envoy:latest'",
        "envoy:down": "bash -c 'docker stop trading-post-mirror-envoy && docker rm trading-post-mirror-envoy'"
    },
    "dependencies": {
